{
 "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],

 "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },

  "env": {
      "browser": true,
  },

  "rules": {
    "react/prop-types": 0
  }
}
